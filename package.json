  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "pack": "tsc --p ./tsconfig-build.json  ",
    "dec": "cp lib/evo-client-lib.d.ts dist/evo-client-lib.d.ts",
    "types": "cp -r lib/types dist/types",
    "docs": "cp -r docs dist/docs",
    "build:types": "npm run types && npm run dec && npm run docs",
    "build": "rm -rf dist && npm run pack && vite build && npm run build:types",
